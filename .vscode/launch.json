    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "start"
            ],
            "envFile": "${workspaceFolder}/.env",
            "skipFiles": [
                "<node_internals>/**/*.js"
            ],
            "env": {
              "FORGE_CLIENT_ID": "",
              "FORGE_CLIENT_SECRET": "",
              "FORGE_CALLBACK_URL": "http://localhost:3000/api/auth/callback",
              "SERVER_SESSION_SECRET": "mysecret2"
            }
        }
    ]
}