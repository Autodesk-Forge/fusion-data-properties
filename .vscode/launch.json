        {
          "type": "node",
          "request": "launch",
          "name": "PRD",
          "runtimeExecutable": "npm",
          "runtimeArgs": [
              "start"
          ],
          "envFile": "${workspaceFolder}/.env",
          "skipFiles": [
              "<node_internals>/**/*.js"
          ],
          "env": {
            "FORGE_CLIENT_ID": "",
            "FORGE_CLIENT_SECRET": "",
